{
  "name": "graphql-svelte",
  "version": "1.2.11",
  "description": "graphql fetcher alternate apollo client",
  "main": "dist",
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage --detectOpenHandles",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "travis-deploy-once": "travis-deploy-once",
    "prepublishOnly": "npm test && npm run build"
  },
  "author": {
    "name": "timeshift92",
    "url": "https://github.com/timeshift92",
    "email": "time_shift92@mail.ru"
  },
  "keywords": [
    "graphql",
    "client",
    "query",
    "mutation",
    "subscription",
    "esm"
  ],
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/src/reportCacheErrors.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/jest": "^26.0.19",
    "@types/koa": "^2.11.6",
    "@types/node": "^14.14.14",
    "apollo-server": "^2.19.0",
    "colors": "^1.4.0",
    "commitizen": "^4.2.2",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "formdata-node": "^2.4.0",
    "graphql": "^15.4.0",
    "graphql-api-koa": "^6.0.0",
    "graphql-tools": "7.0.2",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-config": "^24.9.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "lint-staged": "^10.5.3",
    "lodash.camelcase": "^4.3.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "semantic-release": "^17.3.0",
    "shelljs": "^0.8.4",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^3.7.4"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=8.5"
  },
  "dependencies": {
    "@sindresorhus/fnv1a": "^2.0.1",
    "backo": "^1.1.0",
    "mitt": "^2.1.0",
    "symbol-observable": "^3.0.0"
  },
  "peerDependencies": {
    "svelte": "^3.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeshift92/graphql-svelte.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timeshift92/graphql-svelte/issues"
  },
  "homepage": "https://github.com/timeshift92/graphql-svelte#readme"
}
