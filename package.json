{
  "name": "graphql-svelte",
  "version": "1.2.8",
  "description": "graphql fetcher alternate apollo client",
  "main": "dist/graphql-svelte.js",
  "scripts": {
    "clean": "rm -rf public/dist*",
    "build": "tsc && rollup -c",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:tap ",
    "test:eslint": "eslint . --ext js",
    "test:prettier": "prettier '**/*.{json,yml,md,svelte}' ",
    "test:tap": "tap   --test-ignore=src --100 --nyc-arg=--exclude='src/test/helpers/**' src/test/*.js",
    "prepare": "npm run prepare:clean && npm run prepare:js && npm run prepare:js && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf universal server test",
    "prepare:js": "echo js",
    "prepare:jsdoc": "echo doc",
    "prepare:prettier": "prettier 'src/**/*.{js,svelte}' readme.md --write",
    "prepublishOnly": "npm test"
  },
  "author": {
    "name": "timeshift92",
    "url": "https://github.com/timeshift92",
    "email": "time_shift92@mail.ru"
  },
  "keywords": [
    "graphql",
    "client",
    "query",
    "mutation",
    "subscription",
    "esm"
  ],
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "abstract-state-router": "^6.1.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "babel-preset-env": "7.0.0-beta.3",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.0.6",
    "eslint": "^7.16.0",
    "eslint-config-env": "^15.0.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "formdata-node": "^2.4.0",
    "graphql": "^15.4.0",
    "graphql-api-koa": "^6.0.0",
    "graphql-tools": "7.0.2",
    "husky": "5.0.0-alpha.6",
    "jsdoc-md": "^8.0.0",
    "koa": "^2.13.0",
    "koa-bodyparser": "^4.3.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.35.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.10",
    "size-limit": "^4.9.1",
    "svelte": "3.31.0",
    "tap": "^14.11.0",
    "typescript": "^4.1.3"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=8.5"
  },
  "dependencies": {
    "backo": "^1.1.0",
